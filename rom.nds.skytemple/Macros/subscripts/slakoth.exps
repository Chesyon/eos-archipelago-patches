            english=" Getting excited over the planet's\nparalysis takes too much effort...",
            french=" A quoi bon s'agiter pour éviter\nla Paralysie de la Planète? C'est beaucoup\ntrop d'efforts...",
            german=" Im Hinblick auf die Lähmung des\nPlaneten nervös zu werden, erfordert zu viel\nAnstrengung.",
            italian=" Agitarsi per la paralisi del\npianeta è troppo faticoso...",
            spanish=" Inquietarnos por la parálisis del\nplaneta requeriría demasiado esfuerzo...",
        });
        message_Close();
        jump @label_130;
    } elseif ($DUNGEON_CONQUEST_LIST[20]) {
        // [14, 5], Northern Desert Dead End
        with(actor ACTOR_PLAYER) {
            SetAnimation(2);
        }
        with(actor ACTOR_NPC_NAMAKERO) {
            SetAnimation(2);
        }
        with(actor ACTOR_ATTENDANT1) {
            SetAnimation(2);
        }
        with(actor ACTOR_ATTENDANT1) {
            Turn2DirectionLives(4, 10, ACTOR_NPC_NAMAKERO);
        }
        message_SetFace(ACTOR_NPC_NAMAKERO, FACE_NORMAL, FACE_POS_STANDARD);
