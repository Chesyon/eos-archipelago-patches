            SetAnimation(2);
        }
        with(actor ACTOR_ATTENDANT1) {
            Turn2DirectionLives(4, 10, ACTOR_NPC_NAMAKERO);
        }
        message_SetFace(ACTOR_NPC_NAMAKERO, FACE_NORMAL, FACE_POS_STANDARD);
        message_Talk({
            english=" Just lying here takes too\nmuch effort...",
            french=" Rien que rester affalés ici,\nc'est déjà trop crevant pour nous...",
            german=" Nur hier herumzuliegen\nerfordert schon zu viel Anstrengung.",
            italian=" Stare stesi qui è già anche\ntroppo faticoso per noi...",
            spanish=" Solo estar aquí parados ya nos\nsupone un gran esfuerzo...",
        });
        message_Close();
        jump @label_130;
    } elseif ($DUNGEON_ARRIVE_LIST[0]) {
        // [7, 6], Team Skull Live Here...
        with(actor ACTOR_PLAYER) {
            SetAnimation(2);
        }
        with(actor ACTOR_NPC_YADON) {
            SetAnimation(2);
        }
        with(actor ACTOR_NPC_NAMAKERO) {
            SetAnimation(2);
        }
        with(actor ACTOR_ATTENDANT1) {
            SetAnimation(2);
        }
