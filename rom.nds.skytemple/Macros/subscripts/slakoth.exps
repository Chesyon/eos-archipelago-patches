            SetAnimation(2);
        }
        with(actor ACTOR_ATTENDANT1) {
            Turn2DirectionLives(4, 10, ACTOR_NPC_NAMAKERO);
        }
        message_SetFace(ACTOR_NPC_NAMAKERO, FACE_NORMAL, FACE_POS_STANDARD);
        message_Talk({
            english=" What's a Recycle Shop?",
            french=" C'est quoi une Bourse d'Echange?",
            german=" Was ist ein\nWiederverwertungsladen?",
            italian=" Che cos'è un Centro Riciclo?",
            spanish=" ¿Qué es el [CS:K]Reciclaje Explorador[CR]?",
        });
        message_Talk({
            english=" Learning things takes too\nmuch effort...",
            french=" Apprendre de nouveaux trucs,\nc'est trop fatigant...",
            german=" Neues zu lernen ist wirklich\nsehr anstrengend...",
            italian=" Imparare è troppo faticoso...",
            spanish=" Aprender cosas me cansa\nlo indecible...",
        });
        message_Close();
        jump @label_130;
    } elseif ($DUNGEON_OPEN_LIST[35]) {
        // [23, 2], Skull Ambush Torkoal
        with(actor ACTOR_TALK_MAIN) {
            ExecuteCommon(CORO_LIVES_REPLY_NORMAL, 0);
        }
        with(actor ACTOR_TALK_SUB) {
            ExecuteCommon(CORO_LIVES_REPLY_NORMAL, 0);
        }
