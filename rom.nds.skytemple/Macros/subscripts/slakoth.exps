            Turn2DirectionLives(4, 10, ACTOR_NPC_NAMAKERO);
        }
        message_SetFace(ACTOR_NPC_YADON, FACE_NORMAL, FACE_POS_STANDARD);
        message_Talk({
            english=" Other teams are losing their\nstomachs for exploring because of the awful\nstench lately...",
            french=" Ces derniers temps, certaines\néquipes ne vont plus en exploration à cause\nde cette puanteur infecte...",
            german=" Andere Teams sind schon so\nbenebelt von dem fürchterlichen Gestank,\ndass sie nicht mehr auf Erkundung gehen.",
            italian=" Le altre squadre ultimamente\nstanno faticando molto a esplorare a causa di\nquella puzza insopportabile...",
            spanish=" A otros equipos se les ha\nrevuelto el estómago por explorar con\nel hedor que hay últimamente...",
        });
