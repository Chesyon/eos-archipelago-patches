            Turn2DirectionLives(4, 10, ACTOR_NPC_AABOKKU);
        }
        message_SetFace(ACTOR_NPC_AABOKKU, FACE_NORMAL, FACE_POS_STANDARD);
        message_Talk({
            english=" We've been exploring\n[CS:P]Zero Isle[CR], but...",
            french=" Nous avons exsssploré\nl'[CS:P]Ile Zéro[CR]...",
            german=" Wir haben die [CS:P]Null-Insel[CR]\nerkundet, aber...",
            italian=" Abbiamo esssplorato l'[CS:P]Isssola[CR]\n[CS:P]Zero[CR], ma...",
            spanish=" Hemosss essstado explorando la\n[CS:P]Isla Cero[CR], pero...",
