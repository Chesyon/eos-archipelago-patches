import "hints.exps";
macro WeavileMain() {
    ~CheckHints();
    if ($DUNGEON_CONQUEST_LIST[61]) {
        // [29, 67], Aegis Cave Complete!
        with(actor ACTOR_PLAYER) {
            SetAnimation(2);
        }
        with(actor ACTOR_NPC_MANYUURA) {
            SetAnimation(2);
        }
        with(actor ACTOR_NPC_AABOKKU) {
            SetAnimation(2);
        }
        with(actor ACTOR_NPC_DORAPION) {
            SetAnimation(2);
        }
        with(actor ACTOR_ATTENDANT1) {
            SetAnimation(2);
        }
        with(actor ACTOR_ATTENDANT1) {
            Turn2DirectionLives(4, 10, ACTOR_NPC_MANYUURA);
        }
        message_SetFace(ACTOR_NPC_MANYUURA, FACE_NORMAL, FACE_POS_STANDARD);
        message_Talk({
            english=" You're making our Team [CS:X]AWD[CR]\nlook bad!",
            french=" A cause de vos exploits, vous\njetez le discrédit sur notre Equipe [CS:X]DDA[CR]!",
            german=" Ihr lasst unser Team [CS:X]SPA[CR]\nschlecht aussehen!",
            italian=" State mettendo in cattiva luce il\nTeam [CS:X]AWD[CR]!",
            spanish=" ¡El [CS:X]Equipo WAD[CR] no está a\nla altura de vuestro equipo! ¡Qué palo!",
        });
        message_Talk({
            english=" The glory is ours next time!",
            french=" La prochaine fois, à nous la\ngloire!",
            german=" Nächstes Mal werden wir\ntriumphieren!",
            italian=" La prossima volta saremo noi a\nprimeggiare!",
            spanish=" ¡Pero os superaremos la\npróxima vez!",
        });
        message_Close();
        §label_130;
        JumpCommon(CORO_END_TALK);
    } elseif ($DUNGEON_CONQUEST_LIST[45]) {
        // [29, 5], Graduating from the guild
        with(actor ACTOR_TALK_MAIN) {
            ExecuteCommon(CORO_LIVES_REPLY_NORMAL, 0);
        }
        with(actor ACTOR_TALK_SUB) {
            ExecuteCommon(CORO_LIVES_REPLY_NORMAL, 0);
        }
        with(actor ACTOR_ATTENDANT1) {
            SetAnimation(2);
        }
        with(actor ACTOR_ATTENDANT1) {
            Turn2DirectionLives(4, 10, ACTOR_NPC_MANYUURA);
        }
        message_SetFace(ACTOR_NPC_MANYUURA, FACE_NORMAL, FACE_POS_STANDARD);
        message_Talk({
            english=" Well if it isn't you two.[K]\nIt's been a long time.",
            french=" C'est vous.[K] Ça faisait\nlongtemps.",
            german=" Wenn das mal nicht ihr zwei\nseid. [K]Ist schon eine Weile her.",
            italian=" Ma siete proprio voi due![K]\nNe è passato di tempo!",
            spanish=" Mira a quién tenemos aquí.[K]\nHa pasado mucho tiempo.",
