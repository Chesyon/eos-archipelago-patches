                {
                    english=" [CS:P]Zero Isle[CR]?",
                    french=" L'[CS:P]Ile Zéro[CR]?",
                    german=" Die [CS:P]Null-Insel[CR]?",
                    italian=" L'[CS:P]Isola Zero[CR]?",
                    spanish=" ¿La [CS:P]Isla Cero[CR]?",
                }
            default:
                {
                    english=" [CS:P]Zero Isle[CR]?",
                    french=" L'[CS:P]Ile Zéro[CR]?",
                    german=" Die [CS:P]Null-Insel[CR]?",
                    italian=" L'[CS:P]Isola Zero[CR]?",
                    spanish=" ¿La [CS:P]Isla Cero[CR]?",
                }
        }
        CallCommon(CORO_MESSAGE_CLOSE_WAIT_FUNC);
        se_Play(8962);
        with(actor ACTOR_ATTENDANT1) {
            SetEffect(EFFECT_QUESTION_MARK, 3);
        }
        with(actor ACTOR_ATTENDANT1) {
            WaitEffect();
        }
        WaitExecuteLives(ACTOR_ATTENDANT1);
        message_SetFace(ACTOR_ATTENDANT1, FACE_WORRIED, FACE_POS_BOTTOM_R_FACEINW);
        message_SwitchTalk ($PARTNER_TALK_KIND) {
            case 1:
                {
                    english=" No. Should I?",
                    french=" Non, jamais entendu parler.\nPourquoi?",
                    german=" Nein. Sollte ich diesen Ort\nkennen?",
                    italian=" No. Dovrei conoscerla?",
                    spanish=" No. ¿Debería?",
                }
            case 2:
                {
                    english=" No. Should I?",
                    french=" Non, jamais entendu parler.\nPourquoi?",
                    german=" Nein. Sollte ich diesen Ort\nkennen?",
                    italian=" No. Dovrei conoscerla?",
                    spanish=" No. ¿Debería?",
                }
            default:
                {
                    english=" No. Should I?",
                    french=" Non, jamais entendu parler.\nPourquoi?",
                    german=" Nein. Sollte ich diesen Ort\nkennen?",
                    italian=" No. Dovrei conoscerla?",
                    spanish=" No. ¿Debería?",
                }
