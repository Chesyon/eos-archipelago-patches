                english=" We need to keep moving!",
                french=" Il faut continuer à avancer!",
                german=" Wir müssen weiter!",
                italian=" Forza, muoviamoci!",
                spanish=" ¡Hay que seguir adelante!",
            });
            message_Close();
            jump @label_130;
        } else {
            $SCENARIO_TALK_BIT_FLAG[27] = 1;
