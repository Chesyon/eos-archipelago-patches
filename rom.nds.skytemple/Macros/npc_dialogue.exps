
macro ChatotDialogue() {
    with (actor ACTOR_TALK_MAIN) {
        ExecuteCommon(CORO_LIVES_REPLY_NORMAL, 0);
    }
    with (actor ACTOR_TALK_SUB) {
        ExecuteCommon(CORO_LIVES_REPLY_NORMAL, 0);
    }
    with (actor ACTOR_ATTENDANT1) {
        SetAnimation(2);
    }
    with (actor ACTOR_ATTENDANT1) {
        Turn2DirectionLives(4, 10, ACTOR_NPC_PERAPPU);
    }
    if ($DUNGEON_OPEN_LIST[4]) {
        if ($DUNGEON_CONQUEST_LIST[5]) {
        } elseif ($DUNGEON_ENTER_INDEX == 4 || $DUNGEON_ENTER_INDEX == 5) {
            message_SetFace(ACTOR_NPC_PERAPPU, FACE_NORMAL, FACE_POS_STANDARD);
            message_Talk({
                english=" Ah, you two. Are you more or\nless settled into the guild?",
                french=" Ah, vous deux. Avez-vous fini\nde vous installer parmi nous à la Guilde?",
                german=" Ah, ihr zwei. Habt ihr euch mehr\noder minder in der Gilde eingelebt?",
                italian=" Ah, voi due. Vi sentite a vostro\nagio all\'interno della Gilda?",
                spanish=" Ah, aquí estáis. ¿Ya os sentís a\ngusto en el [CS:N]Pokégremio[CR]?",
            });
            message_Talk({
                english=" If you\'ve finally made yourself\ncomfortable, then you should get down to work.",
                french=" Si vous êtes enfin à votre aise\nici, il est temps de vous mettre au travail.",
                german=" Sobald ihr es euch gemütlich\ngemacht habt, solltet ihr an die Arbeit gehen.",
                italian=" Se è tutto a posto, potete\ncominciare a darvi da fare.",
                spanish=" Si ya os habéis acomodado, es\nhora de ponerse a trabajar.",
            });
            message_Talk({
                english=" It will be good to earn loads of\nmoney for yourselves...and the guild! ♪",
                french=" Il serait bon que vous gagniez\nbeaucoup d\'argent... à partager avec la Guilde,\nbien entendu! ♪",
                german=" Es wird sich auszahlen, einen\nHaufen Geld für euch selbst zu verdienen...\nUnd für die Gilde! ♪",
                italian=" Potrete guadagnare molti soldi\nper voi... e per la Gilda! ♪",
                spanish=" Así podréis ganar un montón de\ndinero para llenar vuestros bolsillos...\n¡y los del [CS:N]Pokégremio[CR]! ♪",
            });
