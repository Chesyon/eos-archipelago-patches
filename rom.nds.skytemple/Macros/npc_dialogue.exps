            return;
        } else {
            message_SetFace(ACTOR_NPC_PERAPPU, FACE_NORMAL, FACE_POS_STANDARD);
            message_Talk({
                english=" You two![K] Did [CS:N]Bidoof[CR] give you a\ntour of the premises?",
                french=" Vous deux![K] [CS:N]Keunotor[CR]\nvous a-t-il fait faire le tour du propriétaire?",
                german=" Ihr zwei![K] Hat [CS:N]Bidiza[CR] euch auf\ndem Gelände herumgeführt?",
                italian=" Ehi voi![K] [CS:N]Bidoof[CR] vi ha fatto fare\nun giro del posto?",
                spanish=" ¡Eh, escuchadme![K] ¿Os ha llevado\n[CS:N]Bidoof[CR] a ver las instalaciones del [CS:N]Pokégremio[CR]?",
            });
