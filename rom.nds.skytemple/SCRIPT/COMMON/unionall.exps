coro HANYOU_SAVE_FUNC {
    if ( $SCENARIO_MAIN_BIT_FLAG[7] ) {
        §label_31;
        message_Mail({
            english="[CN]Would you like to save your adventure?",
            french="[CN]Voulez-vous sauvegarder votre aventure?",
            german="[CN]Möchtest du dein Abenteuer speichern?",
            italian="[CN]Vuoi salvare l\'avventura?",
            spanish="[CN]¿Quieres guardar la partida?",
        });
        switch ( message_SwitchMenu(0, 3) ) {
            case menu({
                english="Save and Continue",
                french="Sauvegarder et continuer",
                german="Speichern und fortsetzen",
                italian="Salva e continua",
                spanish="Guardar y continuar",
            }):
            default:
                §label_32;
                message_Close();
                if ( $SCENARIO_MAIN_BIT_FLAG[7] ) {
                    §label_35;
                    $SCENARIO_MAIN_BIT_FLAG[6] = 1;
                    switch ( message_Menu(MENU_SAVE_MENU) ) { }
                    $SCENARIO_MAIN_BIT_FLAG[6] = 0;
                    $SCENARIO_MAIN_BIT_FLAG[7] = 0;
                    return;
                } else {
                    debug_Print('COMPULSORY_SAVE');
                    $GROUND_PLACE = 307;
                    jump @label_35;
                }
                break;
            case menu({
                english="Save and Return to Top Menu",
                french="Sauvegarder et revenir au menu",
                german="Speichern und zum Hauptmenü",
                italian="Salva e torna al menu principale",
                spanish="Guardar y volver al menú",
            }):
                message_Close();
                $SCENARIO_MAIN_BIT_FLAG[6] = 1;
                switch ( message_Menu(MENU_SAVE_MENU) ) { }
                bgm_FadeOut(60);
                screen2_FadeOut(0, 30);
                screen_FadeOut(1, 30);
                switch ( ProcessSpecial(PROCESS_SPECIAL_JUMP_TO_TITLE_SCREEN, 0, 0) ) { }
                return;
            case menu({
                english="Don\'t Save",
                french="Ne pas sauvegarder",
                german="Nicht speichern",
                italian="Non salvare",
                spanish="No guardar",
            }):
                §label_33;
                message_Close();
                $SCENARIO_MAIN_BIT_FLAG[7] = 0;
                return;
        }
    } else {
        message_Mail({
            english="[CN]Would you like to save your adventure?",
            french="[CN]Voulez-vous sauvegarder votre aventure?",
            german="[CN]Möchtest du dein Abenteuer speichern?",
            italian="[CN]Vuoi salvare l\'avventura?",
            spanish="[CN]¿Quieres guardar la partida?",
        });
        switch ( message_SwitchMenu(0, 2) ) {
            case menu({
                english="Yes",
                french="Oui",
                german="Ja",
                italian="Sì",
                spanish="Sí",
            }):
                jump @label_32;
            case menu({
                english="No",
                french="Non",
                german="Nein",
                italian="No",
                spanish="No",
            }):
                jump @label_33;
            default:
                jump @label_31;
        }
    }
}

