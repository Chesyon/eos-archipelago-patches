    // Sky Peak Station stuff
    §sky_peak_closed;
    $EVENT_LOCAL = 0;
    if ($EVENT_LOCAL == 1) { // Only a jump can enter this branch!
        §sky_peak_closed_request;
        $EVENT_LOCAL = 1;
    }
    ~PeakBlock();
    JumpCommon(CORO_EVENT_HA_SAME);
    return;
    §check_all_treasures;
    $EVENT_LOCAL = 0;
    if ($SCENARIO_SUB3 == 255) { // All the other treasures
        switch(scn($SCENARIO_SUB3)[1]) { // Rayquaza
            case &<< 1:
                switch(scn($SCENARIO_SUB3)[1]) { // Mew
                    case &<< 3:
                        item_Set(0,43,0);
                        me_Play(6);
                        message_Mail("Congratulations! You\'ve claimed all 7 of\nthe legendary treasure checks!");
